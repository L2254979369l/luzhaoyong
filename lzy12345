#include<stdio.h>
int main()
{
	int i,a[10],min;
	printf("enter ten numbers:\n");
	for(i=0;i<10;i++)
		scanf("%d",&a[i]);
	printf("now the number you input is as follow:\n");
	for(i=0;i<10;i++)
		printf("%d ",a[i]);
	min=a[0];
	for(i=1;i<9;i++)
		if(a[i]<a[0])
			min=a[i];
	printf("\nthe minist number is %d\n",min);
	return 0;
}
